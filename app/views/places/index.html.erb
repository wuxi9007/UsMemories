<h1>UsPlaces</h1>
<div>
  <% @places.each do |p| %>
    <div>
      <p><%= p.name %>, <%= p.longitude %>, <%= p.latitude %></p>
      <div>
        <iframe width="300" height="300" frameborder="0" style="border:0"
         src="https://www.google.com/maps/embed/v1/place?key=<%= Rails.application.credentials.google_maps_api_key %>&q=<%= p.latitude %>,<%= p.longitude %>"
         allowfullscreen>
        </iframe>
      </div>
    </div>
  <% end %>
</div>
